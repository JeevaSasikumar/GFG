//**************************************************************************************
#include <stdio.h>


int main() {
	int t,k;
	scanf("%d",&t);
	scanf("%d",&k);
	
	int a[t];
	for(int i=0;i<t;i++)
	    scanf("%d",&a[i]);
	
    int max=0;
    for(int j=1;j<=100000;j++)
    {
        long s=0;
        for(int k=0;k<t;k++)
        {
            int u=a[k]-j;
            if(u<0)
            {
                u=0;
            }
            s+=u;
        }
        if(s>=k)
            max=j;
        if(s<k)
            break;
    }
    printf("%d",max);
	return 0;
}
