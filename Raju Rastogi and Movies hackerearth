v=list(map(int, input().split()))
t=v[0]
k=v[1]
mx=0
a=list(map(int, input().split()))
for i in range(1,10):
    s=0
    for j in range(0,len(a)):
        u=a[j]-i
        if(u<0):
            u=0
        s+=u
    if(s>=k):
        mx=i
    if(s<k):
        break
print(mx)
        
        

    //************************************************************
    #include <iostream>
using namespace std;

int main() {
	int t,k;
	cin>>t;
	cin>>k;
	int a[t];
	for(int i=0;i<t;i++)
	    cin>>a[i];
    int max=0;
    for(int j=1;j<=100000;j++)
    {
        long s=0;
        for(int k=0;k<t;k++)
        {
            int u=a[k]-j;
            if(u<0)
            {
                u=0;
            }
            s+=u;
        }
        if(s>=k)
            max=j;
        if(s<k)
            break;
    }
    cout<<max;
	return 0;
}
